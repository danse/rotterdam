directions a b = a &> b :: Node -> Node -> [Link]
connections a b = a &- b :: Node -> Node -> [Link]
join a b = a & b :: Node -> Node -> Link
prepend chain link = link.hook = chain
link = identity :: text, offset :: integer, hook :: link



